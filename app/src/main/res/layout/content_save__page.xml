<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#A5A5A5"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".Save_Page"
    tools:showIn="@layout/activity_save__page">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="751dp"
        android:layout_height="159dp"
        android:layout_marginStart="177dp"
        android:layout_marginEnd="176dp"
        android:fontFamily="@font/bolt_regular"
        android:text="Any RELEVANT info can be put in the Minnesota Bluff Scouting channel on slack."
        android:textAlignment="center"
        android:textColor="#000"
        android:textSize="45sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.212" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="750dp"
        android:layout_height="95dp"
        android:layout_marginStart="177dp"
        android:layout_marginEnd="176dp"
        android:fontFamily="@font/bolt_regular"
        android:text="- Team number"
        android:textAlignment="center"
        android:textColor="#000"
        android:textSize="45sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.471"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.522" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="742dp"
        android:layout_height="157dp"
        android:layout_marginStart="177dp"
        android:layout_marginEnd="176dp"
        android:fontFamily="@font/bolt_regular"
        android:text="Information should include:"
        android:textAlignment="center"
        android:textColor="#000"
        android:textSize="45sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.457"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.453" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="750dp"
        android:layout_height="95dp"
        android:layout_marginStart="177dp"
        android:layout_marginEnd="176dp"
        android:fontFamily="@font/bolt_regular"
        android:text="- Relevant info from match"
        android:textAlignment="center"
        android:textColor="#000"
        android:textSize="45sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.471"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.582" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="608dp"
        android:fontFamily="@font/bolt_regular"
        android:text="Submit Data"
        android:textColor="#000"
        android:textSize="60sp"
        app:layout_constraintBottom_toTopOf="@+id/Submit_To_Store_B"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Submit_To_Store_B"
        android:layout_width="539dp"
        android:layout_height="283dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:fontFamily="@font/bolt_regular"
        android:text="Submit Data"
        android:textSize="90sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.893" />

    <Button
        android:id="@+id/Delete_Data_B"
        android:layout_width="177dp"
        android:layout_height="61dp"
        android:text="Delete Data"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.025"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.986" />

</android.support.constraint.ConstraintLayout>